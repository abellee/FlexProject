package com.plter.air.windows.utils
{
	
	
	/**
	 * 指定在调用本机命令时是否显示命令窗口的常量
	 * @author 
	 */
	public class ShowCmdWindow
	{
		
		
		/**
		 * 在调用命令时隐藏命令窗体 
		 */
		public static const HIDE:String="hide";
		
		
		/**
		 * 在调用命令时显示命令窗体 
		 */
		public static const SHOW:String="show";
	}
}